{
    "manifest_version": 2,

    "name": "Youtube Ad Blocker",
    "description": "Youtube Ad Blocker 1",
    "version": "1.0",

    "content_scripts": [
        {
            "js": ["script.js"],
            "run_at": "document_idle",
            "matches": ["<all_urls>"]
        }
    ],
    "permissions": [
        "tabs",
        "activeTab"
    ]
}